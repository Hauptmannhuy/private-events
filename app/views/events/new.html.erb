<%= form_with model: @event, method: :post do |form| %>
<%= form.label "name"%>
  <%= form.text_field "name"%>
  <%= form.label "date"%>
  <%= form.date_field "date"%>
  <%= form.label "location"%>
  <%= form.text_field "location"%>
  <%= form.label "description"%>
  <%= form.text_field "description" %>
  <%= form.submit "Submit" %>
<% end %>